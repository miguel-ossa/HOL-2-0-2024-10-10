      *--- Modificacion en DB2                                      ---
      *----------------------------------------------------------------
       7300-DB2-MODIFICA.
      *
      *****EXEC SQL
      *****     UPDATE AYUDAS
      *****        SET
      *****          TRANSACCION = :AY-TRANSACCION
      *****        , PAGINA      = :AY-PAGINA
      *****        , L01         = :AY-L01
      *****        , L02         = :AY-L02
      *****        , L03         = :AY-L03
      *****        , L04         = :AY-L04
      *****        , L05         = :AY-L05
      *****        , L06         = :AY-L06
      *****        , L07         = :AY-L07
      *****        , L08         = :AY-L08
      *****        , L09         = :AY-L09
      *****        , L10         = :AY-L10
      *****        , L11         = :AY-L11
      *****        , L12         = :AY-L12
      *****        , L13         = :AY-L13
      *****        , L14         = :AY-L14
      *****        , L15         = :AY-L15
      *****        , L16         = :AY-L16
      *****        WHERE
      *****              TRANSACCION = :AY-TRANSACCION
      *****          AND PAGINA      = :AY-PAGINA
      *****END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
