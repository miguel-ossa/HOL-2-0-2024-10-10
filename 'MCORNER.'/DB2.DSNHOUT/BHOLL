      *----------------------------------------------------------------
      *--- Cuenta los clientes existentes y los muestra en pantalla ---
      *----------------------------------------------------------------
       7800-CUENTA-CLIENTES.
      *
      *****EXEC SQL
      *****     SELECT COUNT (*) INTO :DB2-COUNT1
      *****     FROM CLIENTES
      *****END-EXEC.
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST58.
      *
       7800-CUENTA-CLIENTES-EXIT.
           EXIT.
      *----------------------------------------------------------------
      *--- Cierra el cursor que este abieerto                       ---
      *----------------------------------------------------------------
       7900-CLOSE-CURSOR.
      *
           IF HAY-FILTRO-COMIENZA
              IF PARM-CURSOR = 'DA'
      *****        EXEC SQL CLOSE DACOM END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST59
              END-IF
              IF PARM-CURSOR = 'DD'
      *****        EXEC SQL CLOSE DDCOM END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST60
              END-IF
              IF PARM-CURSOR = 'NA'
      *****        EXEC SQL CLOSE NACOM END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST61
              END-IF
              IF PARM-CURSOR = 'ND'
      *****        EXEC SQL CLOSE NDCOM END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST62
              END-IF
              IF PARM-CURSOR = 'AA'
      *****        EXEC SQL CLOSE AACOM END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST63
              END-IF
              IF PARM-CURSOR = 'AD'
      *****        EXEC SQL CLOSE ADCOM END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST64
              END-IF
           END-IF
      *
           IF HAY-FILTRO-CONTIENE
              IF PARM-CURSOR = 'DA'
      *****        EXEC SQL CLOSE DACON END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST65
              END-IF
              IF PARM-CURSOR = 'DD'
      *****        EXEC SQL CLOSE DDCON END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST66
              END-IF
              IF PARM-CURSOR = 'NA'
      *****        EXEC SQL CLOSE NACON END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST67
              END-IF
              IF PARM-CURSOR = 'ND'
      *****        EXEC SQL CLOSE NDCON END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST68
              END-IF
              IF PARM-CURSOR = 'AA'
      *****        EXEC SQL CLOSE AACON END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST69
              END-IF
              IF PARM-CURSOR = 'AD'
      *****        EXEC SQL CLOSE ADCON END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST70
              END-IF
           END-IF
      *
           IF NOT HAY-FILTRO-CONTIENE AND NOT HAY-FILTRO-COMIENZA
              IF PARM-CURSOR = 'DA'
      *****        EXEC SQL CLOSE CURDA END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST71
              END-IF
              IF PARM-CURSOR = 'DD'
      *****        EXEC SQL CLOSE CURDD END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST72
              END-IF
              IF PARM-CURSOR = 'NA'
      *****        EXEC SQL CLOSE CURNA END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST73
              END-IF
              IF PARM-CURSOR = 'ND'
      *****        EXEC SQL CLOSE CURND END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST74
              END-IF
              IF PARM-CURSOR = 'AA'
      *****        EXEC SQL CLOSE CURAA END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST75
              END-IF
              IF PARM-CURSOR = 'AD'
      *****        EXEC SQL CLOSE CURAD END-EXEC
           PERFORM SQL-INITIAL UNTIL SQL-INIT-DONE
           CALL 'DSNHLI' USING SQL-PLIST76
              END-IF
           END-IF.
      *
       7900-CLOSE-CURSOR-EXIT.
           EXIT.
      *--- TXMUL <-
      *----------------------------------------------------------------
      *--- Obtener texto fijo con prog LNKTLB    de tabla: TABLAS   ---
      *----------------------------------------------------------------
       20100-OBTENER-TEXTO.
      *
           MOVE '20100-OBTENER-TEXTO'        TO WA-PARRAFO.
           MOVE DB2-TABLA-PROGRMSG           TO CL-TABLA.
           MOVE LOW-VALUES                   TO CL-CLAVE2.
           MOVE CL-IDIOMA                    TO CL-CLAVE3.
      *    MOVE CS-PGM-TABLA-TITULO          TO CH-LINK.
           CALL 'BLNKTL' USING CH-COMMAREA-HOL.
      *
       20100-OBTENER-TEXTO-EXIT.
           EXIT.
